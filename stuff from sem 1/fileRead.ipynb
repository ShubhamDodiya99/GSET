   "source": [
    "# open both files\n",
    "with open('first.txt','r') as firstfile, open('second.txt','a') as secondfile:\n",
    "\n",
    "    # read content from first file\n",
    "    for line in firstfile:\n",
    "\n",
    "            # append content to second file\n",
    "            secondfile.write(line)"
